{
  "env": {
    "development": {
      "presets": [
        ["es2015", {
          "modules": false
        }], "react", "stage-2"
      ],
      "plugins": ["react-hot-loader/babel", ["transform-runtime", {
        "helpers": false,
        "polyfill": false,
        "regenerator": true,
        "moduleName": "babel-runtime"
      }]]
    },
    "production": {
      "presets": ["es2015", "react", "stage-2"],
      "plugins": [
        ["transform-runtime", {
          "helpers": false,
          "polyfill": false,
          "regenerator": true,
          "moduleName": "babel-runtime"
        }]
      ]
    }
  }
}
